<!DOCTYPE html>
<head>
  <% include partials/head %>
</head>

<% include partials/teacher-sidenav %>
<header>
<%- include('partials/teacher-nav', {page: page}) %>
</header>

<body>
	<main>
		<div class="row">
			<div class="col s12 m6">
				<div class="card">
					<div class="card-content">
						<span class="card-title">Student average scores per Week</span>
						<img class="dashboard_performance_image" src="images/studentaverage.png">
					</div>
				</div>
			</div>
			<div class="col s12 m6">
				<div class="card">
					<div class="card-content">
						<span class="card-title">Attendance and assignment completion</span>
						<img class="dashboard_performance_image" src="images/attendance.png">
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col s12 m12 l12">
				<div class="card">
					<div class="card-content">
						<span class="card-title">Data</span>
						<ul class="collection with-header">
							<li class="collection-header"><h6>All students</h6></li>
							<li class="collection-item">
								<table class="responsive-table bordered highlight">
									<thead>
										<tr>
											<th data-field="id">Name</th>
											<th data-field="name">Avg score Quizzes</th>
											<th data-field="name">Avg score Tutorial</th>
											<th data-field="name">Avg score Assignments</th>
										</tr>
									</thead>

									<tbody>
										<tr>
											<td>Michelle Wong</td>
											<td>74%</td>
											<td>45%</td>
											<td>44%</td>
										</tr>
										<tr>
											<td>John Doe</td>
											<td>85%</td>
											<td>75%</td>
											<td>74%</td>
										</tr>
										<tr>
											<td>Michael Wong</td>
											<td>67%</td>
											<td>74%</td>
											<td>72%</td>
										</tr>
									</tbody>
								</table>

							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</main>
</body>