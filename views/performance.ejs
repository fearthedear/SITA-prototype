<!DOCTYPE html>
<head>
	<% include partials/head %>
</head>

<% include partials/student-sidenav %>
<header>
	<%- include('partials/student-nav', {page: page}) %>
</header>

<body>
	<main>
		<div class="row">
			<div class="col s12 m6">
				<div class="card">
					<div class="card-content">
						<span class="card-title">Performance by Week</span>
						<img class="dashboard_performance_image" src="images/performancebyweek.png">
					</div>
				</div>
			</div>
			<div class="col s12 m6">
				<div class="card">
					<div class="card-content">
						<span class="card-title">Your performance compared to average</span>
						<img class="dashboard_performance_image" src="images/youandaverage.png">
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col s12 m12 l12">
				<div class="card">
					<div class="card-content">
						<span class="card-title">All scores</span>
						<ul class="collection with-header">
							<li class="collection-header"><h6>Weekly Quizzes</h6></li>
							<li class="collection-item">
								<table class="responsive-table bordered highlight">
									<thead>
										<tr>
											<th data-field="id">Quiz #</th>
											<th data-field="name">Score</th>
										</tr>
									</thead>

									<tbody>
										<tr>
											<td>1</td>
											<td>75%</td>
											
										</tr>
										<tr>
											<td>2</td>
											<td>85%</td>
										</tr>
										<tr>
											<td>3</td>
											<td>67%</td>
											
										</tr>
									</tbody>
								</table>

							</li>
						</ul>
						<ul class="collection with-header">
							<li class="collection-header"><h6>Tutorials</h6></li>
							<li class="collection-item">
								<table class="responsive-table bordered highlight">
									<thead>
										<tr>
											<th data-field="id">Tutorial #</th>
											<th data-field="name">Score</th>
										</tr>
									</thead>

									<tbody>
										<tr>
											<td>1</td>
											<td>55%</td>
											
										</tr>
										<tr>
											<td>2</td>
											<td>45%</td>
										</tr>
										<tr>
											<td>3</td>
											<td>87%</td>
											
										</tr>
									</tbody>
								</table>

							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</main>
</body>